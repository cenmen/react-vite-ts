{
	"name": "react-vite-ts",
	"description": "react react-router tailwindcss antd5.x zustand",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --mode dev",
		"tst": "vite --mode tst",
		"prod": "vite --mode prod",
		"build:dev": "vite build --mode dev",
		"build:tst": "vite build --mode tst",
		"build": "vite build --mode prod",
		"preview": "vite preview",
		"prepare": "husky install",
		"type-check": "tsc --noEmit",
		"lint": "pnpm run lint:eslint && pnpm run type-check && pnpm run lint:prettier",
		"lint:eslint": "eslint --fix --max-warnings=0 --ext ts,tsx,js,jsx ./src",
		"lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,jsx,ts,tsx,json,,css,html,md}\""
	},
	"husky": {
		"hooks": {
			"pre-commit": [
				"lint-staged"
			]
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx}": "lint"
	},
	"dependencies": {
		"@ant-design/icons": "^5.3.0",
		"antd": "^5.14.0",
		"axios": "^1.6.7",
		"dayjs": "^1.11.10",
		"lodash-es": "^4.17.21",
		"nanoid": "^5.0.5",
		"qs": "^6.11.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-lottie": "^1.2.4",
		"react-router-dom": "^6.22.0",
		"zustand": "^4.5.0"
	},
	"devDependencies": {
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.11.16",
		"@types/qs": "^6.9.11",
		"@types/react": "^18.2.54",
		"@types/react-dom": "^18.2.18",
		"@types/react-lottie": "^1.2.10",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"husky": "^9.0.10",
		"lint-staged": "^15.2.2",
		"postcss": "^8.4.34",
		"prettier": "^3.2.5",
		"rollup-plugin-visualizer": "^5.12.0",
		"sass": "^1.70.0",
		"tailwindcss": "^3.4.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-html": "^3.2.2"
	}
}
